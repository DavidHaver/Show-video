<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows10.0.17763.0</TargetFramework>
    <RootNamespace>show_video</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <Platforms>AnyCPU;x64</Platforms>
    <StartupObject>show_video.Program</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\DrawVideoCLR\DrawVideoCLR.vcxproj" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="DrawVideoCLR">
      <HintPath>x64\Debug\DrawVideoCLR.dll</HintPath>
		<HintPath>x64\Release\DrawVideoCLR.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <None Update="x64\Debug\Ijwhost.dll">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>